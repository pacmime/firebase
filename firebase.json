{
  "hosting": {
    "public": ".",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**",
      "**/bower/**",
      "**/bower_components/**",
      "sob2-src"
    ],
    "rewrites": [ 
       {
  	"source": "sob2/**",
  	"destination": "/sob2/index.html"
       },
       {
  	"source": "sob/**",
  	"destination": "/sob/index.html"
       }
    ]
  }
}
